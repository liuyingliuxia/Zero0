<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorAccent">#FF4081</color>
    <color name="colorPrimary">#3F51B5</color>
    <color name="colorPrimaryDark">#303F9F</color>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <dimen name="shadow_width">15dp</dimen>
    <dimen name="slidingmenu_offset">100dp</dimen>
    <item name="TAG_PRESENTER" type="id"/>
    <item name="TAG_ROOTVIEW" type="id"/>
    <item name="selected_view" type="id"/>
    <string name="server_type" translatable="false">prod</string>
    <declare-styleable name="CircleFlowIndicator">
        <attr format="color" name="activeColor"/>
        <attr format="color" name="inactiveColor"/>
        <attr format="dimension" name="radius"/>
        <attr format="boolean" name="centered"/>
        <attr format="integer" name="fadeOut"/>
        <attr name="inactiveType">
            <flag name="stroke" value="0"/>
            <flag name="fill" value="1"/>
        </attr>
        <attr name="activeType">
            <flag name="stroke" value="0"/>
            <flag name="fill" value="1"/>
        </attr>
        <attr format="dimension" name="circleSeparation"/>
        <attr format="dimension" name="activeRadius"/>
    </declare-styleable>
    <declare-styleable name="SlidingMenu">
        <attr name="mode">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
        </attr>
        <attr format="reference" name="viewAbove"/>
        <attr format="reference" name="viewBehind"/>
        <attr format="dimension" name="behindOffset"/>
        <attr format="dimension" name="behindWidth"/>
        <attr format="float" name="behindScrollScale"/>
        <attr name="touchModeAbove">
            <enum name="margin" value="0"/>
            <enum name="fullscreen" value="1"/>
        </attr>
        <attr name="touchModeBehind">
            <enum name="margin" value="0"/>
            <enum name="fullscreen" value="1"/>
        </attr>
        <attr format="reference" name="shadowDrawable"/>
        <attr format="dimension" name="shadowWidth"/>
        <attr format="boolean" name="fadeEnabled"/>
        <attr format="float" name="fadeDegree"/>
        <attr format="boolean" name="selectorEnabled"/>
        <attr format="reference" name="selectorDrawable"/>
    </declare-styleable>
    <declare-styleable name="ViewFlow">
        <attr format="integer" name="sidebuffer"/>
    </declare-styleable>
</resources>